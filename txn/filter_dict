def filter_dict(dictionary, keys_to_keep):
    """Filters a dictionary, keeping only the specified keys.

    Args:
        dictionary: The dictionary to filter.
        keys_to_keep: A set of keys to retain.

    Returns:
        A new dictionary containing only the specified keys and their values.
    """

    return {key: dictionary[key] for key in keys_to_keep if key in dictionary}
